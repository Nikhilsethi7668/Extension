{
  "name": "autobridge-extension",
  "displayName": "AutoBridge Pro",
  "version": "2.0.0",
  "description": "AI-powered vehicle marketplace automation with human-mimicry posting",
  "author": "AutoBridge",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "plasmo": "^0.89.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@mui/material": "^6.1.9",
    "@mui/icons-material": "^6.1.9",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.277",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "typescript": "^5.7.2"
  },
  "manifest": {
    "host_permissions": [
      "https://www.autotrader.com/*",
      "https://www.cars.com/*",
      "https://www.cargurus.com/*",
      "https://www.facebook.com/*",
      "https://marketplace.facebook.com/*",
      "https://autobridge-backend.dchatpar.workers.dev/*"
    ],
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs",
      "cookies",
      "notifications",
      "sidePanel"
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    }
  }
}
